<%=render 'header/manager_head'%>
<div class="container">
	<div class="row">
		<div class="col-xs-12">
            <h1>レーベル追加</h1>
            <h2>レーベル名</h2>
            <%= form_for @label,:url => {:action => :create} do |f| %>
                <%= f.text_field :name, autofocus: true %>
                <%= f.submit "更新", class: "btn btn-primary" %>
            <% end %>
            <%= link_to "キャンセル", admin_labels_path,class: "btn btn-success" %>
		</div>
	</div>
</div>
<%=render 'footer/footer'%>
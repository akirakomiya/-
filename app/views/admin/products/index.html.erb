<div class="product_title_group">
<h1>商品一覧</h1>
<%=link_to admin_products_new_path %><div class="to_>add_button">商品を追加する</div><% end %>
</div>
<div class="main_contents_group">
<%=@products.each do |product|%>
    <%=attachment_image_tag product,:jacket_image,:fill,60,60,:fallback: "noimage.png",style:"width:40px;height:40px;"%>
    <table class="alone_contents_group">
        <tr><td><%=product.album%></td></tr>
        <tr><td><%=product.artist%></td></tr>
        <tr><td><%=product.price%></td></tr>
        <tr><td><%=product.stock_quantity%></td></tr>
        <tr><td><%=product.is_selling%></td></tr>
    </table>
    <div class="contents_menu_group">
    <%=link_to "入荷",admin_products_arrive_path(product.id) %>
    <%=link_to "編集",admin_products_edit_path(product.id) %>
    <%=link_to "詳細",admin_products_show_path(product.id) %>
    <%=link_to "削除",admin_products_new_path(product.id),method: :delete,"data-confirm"=>"本当に削除しますか？" %>
    </div>
<% end %>
<%= paginate @products, class: "paginate" %>
</div>
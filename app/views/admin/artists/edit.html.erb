<%=render 'header/manager_head'%>
<div class="container">
	<div class="row">
		<div class="col-xs-12">
			<h1>編集    id:[<%=@artist.id %>]</h1><br><br>
			<h2>アーティスト名</h2>
			<%= form_for @artist,:url => {:action => :update} do |f| %>
			<%= f.text_field :name, autofocus: true %>
			<%= f.submit "更新", class: "btn btn-primary" %>
			<% end %>
			<%= link_to "キャンセル", admin_artists_path,class: "btn btn-success" %>
		</div>
	</div>
</div>
